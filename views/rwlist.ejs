<!DOCTYPE html>
<HTML>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width , initial-scale=1.0">
        <title>List Review</title>
    </head>

    <body>
        <h1>List Review</h1>
        <%let jumlah = 0 %>
        <%for (var i = 0 ; i < data.length ; i ++){ jumlah=+ Number(data[i].rating)} %>
        <h1>Rating Cabang ini : <%= jumlah %> </h1>
        <p1> Anda Login Sebagai </p1>
        <p1><%= session.username %></p1>


        <a href = '/review/info/<%=data[0].Cabang.RestaurantId%>/<%=data[0].CabangId%>/add/user/<%=session.id%>'><button>Add Review</button></a>

        <table border=2 >
            <tr>
                <th>name(username)</th>
                <th>email</th>
                <th>review</th>
                <th>rating</th>
                <th>Action</th>
            </tr>
        <tbody>
            <% data.forEach(el=>{ %>
            <tr>
                <td><%= el.getFullInfo()%></td>
                <td><%= el.User.email%></td>
                <td><%= el.review%></td>
                <td><%= el.rating%></td>
                <td>
                <a href="/review/info/<%=el.Cabang.RestaurantId%>/<%=el.CabangId%>/<%=el.UserId%>/edit"><button>Edit</button></a>
                </td>
            </tr>

            <% }) %>
        </tbody>
        </table>













    </body>






</HTML>